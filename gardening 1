r=int(input())
c=int(input())
m1=[]
m2=[]
m3=[]
for i in range (1,c+1):
    m1.append(i)
for i in range (0,r):
    e=c*i+1
    m2.append(e)
for i in range (0,r*c+1,c):
    m3.append(i)
m3.remove(0)
t_no=int(input())
if t_no in m1:
    print("Yes")
elif t_no in m2:
    print("Yes")
elif t_no in m3:
    print("Yes")
else:
    print("No")
